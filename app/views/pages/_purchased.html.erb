<table class="table table-condensed">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Fecha</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  <% @purchased.each do |sale| %>
    <tr>
      <td><%= link_to sale.item.name, pickup_path(sale.guid), class: "link_custom" %></td>
      <td><%= time_ago_in_words(sale.created_at) %> ago</td>  
    </tr>
	<% end %>
</tbody>
</table>